<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE mapper
  PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN'
  'http://mybatis.org/dtd/mybatis-3-mapper.dtd'>
  
  
<mapper namespace='io.rezoome.manager.database.dao.xml.agency.Dao'>
	<resultMap id="InhaUnivEntity" type="result">
		<result property="user" column="user"/>
		<result property="host" column="host"/>
	</resultMap>


	<select id="getRecord" resultMap="InhaUnivEntity">
		select user,host from mysql.user where user = 'root' limit 1;
	</select>
	
	<select id="getRecords" resultMap="InhaUnivEntity">
		select user,host from mysql.user where user = 'root';
	</select>
</mapper>